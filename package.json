  "bugs": "https://github.com/bjoernek/multi_chain_voting",
  "homepage": "https://github.com/bjoernek/multi_chain_voting",
  "keywords": [
    "dfinity",
    "ic",
    "internet computer",
    "react",
    "template",
    "siwe",
    "sign in with ethereum",
    "tailwindcss",
    "typescript",
    "vite"
  ],
  "type": "module",
  "scripts": {
    "prebuild": "dfx generate",
    "deploy:local": "dfx deploy --network=local",
    "deploy:ic": "dfx deploy --network=ic",
